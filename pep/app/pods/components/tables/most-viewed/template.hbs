<Table
    @containerSelector=".page-content-inner"
    @columns={{this.columns}}
    @rows={{@rows}}
    @hasMoreRows={{@hasMoreRows}}
    @loadMoreRows={{@loadMoreRows}}
    @isLoading={{@isLoading}}
    @updateSorts={{@updateSorts}}
    @enableSort={{true}}
    @sorts={{@sorts}}
    @fillMode="nth-column"
    @fillColumnIndex={{0}}
    @renderAllRows={{this.fastboot.isFastBoot}}
/>